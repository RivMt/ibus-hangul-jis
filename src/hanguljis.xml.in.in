<component>
	<name>org.freedesktop.IBus.HangulJIS</name>
	<description>Korean Component (JIS)</description>
	<exec>${libexecdir}/ibus-engine-hangul-jis --ibus</exec>
	<version>@VERSION@</version>
	<author>Peng Huang &lt;shawn.p.huang@gmail.com&gt;</author>
	<license>GPL</license>
	<homepage>https://github.com/RivMt/ibus-hangul-jis</homepage>
